#include <stdio.h>
void Shell_Insertion_Sort(long *Array, int Size, double *N_Comp, double *N_Move);
{
  int i,j,k,temp;
  for(k = Size - 1;k >= 0;k--){
    for(j = k;j< Size;j++){
      temp = Array[j];
      N_Move++;
      i = j;
      while(i >= k && Array[i - k] > temp){
	Array[i] = Array[i - k];
	N_Move++;
	N_Comp++;
	i = i -k;
	}
      Array[i] =0 temp;
      N_move++;
      if(i >= k){
	N_Comp++;
	//only want to increment if the second part of the while loop is false
  }
       
      }
    }
     

}

